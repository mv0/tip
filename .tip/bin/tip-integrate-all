#!/bin/sh

. $(dirname "$0")/tip-lib

tip-backup || { echo "backup failed"; exit -1; }

AB=$(get_auto_branches_no_latest)

for B in $AB
do
    tip-integrate $B || { echo "Integration of "$B" failed"; exit -1; }
done

tip-integrate auto-generic-ipi-next || { echo "Integration of auto-generic-ipi-next failed"; exit -1; }

tip-integrate-latest

